package ejercicio3;

public class Ejercicio3 {
	

	public static void main (String args [])
	{
		//Crear dos variables de tipo int
		
		int num1;
		num1 = 0; //Dandole valor a la variable
		
		int num2;
		num2 = 0;
		System.out.println("Valor de la variable num1: " + num1);
		System.out.println("Valor de la variable num2: " + num2);
		
		//Crear dos variables de tipo double
		
		double val1;
		double val2;
		
		
	}
		
}
